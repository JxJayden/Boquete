extends layout

block content
  nav.white(role='navigation')
    .nav-wrapper.container
      a#logo-container.brand-logo(href='#') Logo
      ul.right.hide-on-med-and-down
        li
          a(href='#') Navbar Link
      ul#nav-mobile.side-nav
        li
          a(href='#') Navbar Link
      a.button-collapse(href='#', data-activates='nav-mobile')
        i.material-icons menu
  #index-banner.parallax-container
    .section.no-pad-bot
      .container
        br
        br
        h1.header.center.teal-text.text-lighten-2 Parallax Template
        .row.center
          h5.header.col.s12.light A modern responsive front-end framework based on Material Design
        .row.center
          a#download-button.btn-large.waves-effect.waves-light.teal.lighten-1(href='http://materializecss.com/getting-started.html') Get Started
        br
        br
    .parallax
      img(src='img/background1.jpg', alt='Unsplashed background img 1')
  .container
    .section
      // Icon Section
      .row
        .col.s12.m4
          .icon-block
            h2.center.brown-text
              i.material-icons flash_on
            h5.center Speeds up development
            p.light
        .col.s12.m4
          .icon-block
            h2.center.brown-text
              i.material-icons group
            h5.center User Experience Focused
            p.light
        .col.s12.m4
          .icon-block
            h2.center.brown-text
              i.material-icons settings
            h5.center Easy to work with
            p.light
  .parallax-container.valign-wrapper
    .section.no-pad-bot
      .container
        .row.center
          h5.header.col.s12.light A modern responsive front-end framework based on Material Design
    .parallax
      img(src='img/background2.jpg', alt='Unsplashed background img 2')
  .container
    .section
      .row
        .col.s12.center
          h3
            i.mdi-content-send.brown-text
          h4 Contact Us
          p.left-align.light
  .parallax-container.valign-wrapper
    .section.no-pad-bot
      .container
        .row.center
          h5.header.col.s12.light A modern responsive front-end framework based on Material Design
    .parallax
      img(src='img/background3.jpg', alt='Unsplashed background img 3')

  footer.page-footer.teal
    .container
      .row
        .col.l6.s12
          h5.white-text Company Bio
          p.grey-text.text-lighten-4 We are a team of college students working on this project like it's our full time job. Any amount would help support and continue development on this project and is greatly appreciated.
        .col.l3.s12
          h5.white-text Settings
          ul
            li
              a.white-text(href='#!') Link 1
            li
              a.white-text(href='#!') Link 2
            li
              a.white-text(href='#!') Link 3
            li
              a.white-text(href='#!') Link 4
        .col.l3.s12
          h5.white-text Connect
          ul
            li
              a.white-text(href='#!') Link 1
            li
              a.white-text(href='#!') Link 2
            li
              a.white-text(href='#!') Link 3
            li
              a.white-text(href='#!') Link 4
    .footer-copyright
      .container
        a.brown-text.text-lighten-3(href='http://materializecss.com') Materialize
